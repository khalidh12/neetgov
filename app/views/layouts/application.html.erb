<!DOCTYPE html>
<html>
  <head>
    <title>Neetgov</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload', async: true %>
    <title><%= yield(:title) %></title>
    <meta property="og:title" content="<%= yield(:title) %>" />
    <meta name='description' content="<%= yield(:description) %>" >
    <meta property="og:description" content="<%= yield(:description) %>" />
    <meta name="keywords" content="<%= yield(:keywords) %>">
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

     <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css" rel="stylesheet">
     <script type="text/javascript">
        window.setTimeout(function() {
          $(".alert").fadeTo(500, 0).slideUp(500, function(){
            $(this).remove(); 
          });
        }, 5000);
      </script>
      

      <!-- Global site tag (gtag.js) - Google Analytics -->
<script async src="https://www.googletagmanager.com/gtag/js?id=UA-108087534-1"></script>
<script>
window.dataLayer = window.dataLayer || [];
function gtag(){dataLayer.push(arguments);}
gtag('js', new Date());

gtag('config', 'UA-108087534-1');
</script>


        
</head>
  <body>
      <% if user_signed_in? %>
        <%= render :partial => 'layouts/navbar' %>
    	<% else %>
        <%= render :partial => 'layouts/navbar2' %>
    	<%end%>
      
      <div class="container">
      
        <%= yield %>
      </div>
      <%= render :partial =>  "layouts/script" %>
      <br><br><br>
      <%= render :partial =>  "layouts/footer" %>
  </body>
</html>
