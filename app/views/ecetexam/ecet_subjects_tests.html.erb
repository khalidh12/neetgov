	<% i = 1 %>

	<div class="row">
		<div class="col-sm-1 text-center"></div>
			<div class="col-md-10">
				<% if @livetests.empty? %>
					<div class="alert alert-danger">
				      <strong>Sorry! No tests added yet</strong>
				    </div>
						
				<% else %>
						<table class="table table-bordered table-hover">
							<tr>
								<th class="success">Test</th>
								<th class="success">Date</th>
								<th class="success"></th>
							</tr>
							
							<% ad=@livetests.count/2 %>
							<% ad2=@livetests.count/2 %>
							<% count = @livetests.count %>
						<% @livetests.each do |c| %>
							<tr>
								<td><%= c.subject.upcase %> TEST <b><%= i %></b></td>
								<td><%= c.created_at.strftime("#{c.created_at.day.ordinalize}-%b-%Y") %></td>
								<td><a href="/exam/ecet/<%=c.subject %>/<%= c.testcount %>/<%= @subject %>" ><button class="btn btn-primary"> Start Exam </button></a></td>
							</tr>
							<% if i == ad %>
								<tr class="text-center">
									<td colspan="3">
						 				<script async sr="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
						 				 <ins class="adsbygoogle" style="display:block" data-ad-client="ca-pub-7665698555914778" data-ad-slot="8409268847" data-ad-format="link"></ins>
					                	<script>(adsbygoogle = window.adsbygoogle || []).push({});</script>
					                </td> 
					             </tr>
				            <% end %>
							<% i+=1 %>
						<% end %>
						</table>
				<% end %>
